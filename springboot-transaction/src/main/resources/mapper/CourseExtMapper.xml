<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.liyong.test.springboot.mapper.ext.CourseExtMapper">

    <update id="addCourseNumber" >
        update course
        set number = number + 1
        where id = #{courseId}
    </update>

</mapper>
